{
  "name": "soloplander",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "sanity": "cd sanity && npm run dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run sanity\"",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,css,md}\"",
    "prepare": "husky install",
    "db:generate": "drizzle-kit generate:pg",
    "db:push": "drizzle-kit push:pg",
    "sanity:upgrade": "cd sanity && npx sanity upgrade",
    "sanity:check": "cd sanity && npx sanity doctor"
  },
  "dependencies": {
    "@portabletext/react": "^3.2.1",
    "@sanity/icons": "^2.0.0",
    "@sanity/image-url": "^1.0.2",
    "@sanity/vision": "^3.23.4",
    "@vercel/speed-insights": "^1.1.0",
    "drizzle-orm": "^0.29.5",
    "googleapis": "^105.0.0",
    "next": "^14.1.0",
    "next-sanity": "^5.5.10",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.4.0",
    "sanity": "^3.23.4",
    "styled-components": "^6.1.15"
  },
  "devDependencies": {
    "@types/node": "^20.17.12",
    "@types/pg": "^8.11.10",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "drizzle-kit": "^0.20.14",
    "typescript": "^5.3.3"
  }
}
